    "accept": "^3.0.1",
    "aws-sdk": "^2.141.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "serialize-error": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "bundledDependencies": [
    "accept",
    "serialize-error",
